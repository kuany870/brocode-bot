  "devDependencies": {
    "chai": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-standard": "^10.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "run-sequence": "^1.2.2",
    "standard": "^10.0.2"
  },
  "engines": {
    "node": ">= 8.0.0",
